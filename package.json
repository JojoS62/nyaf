{
  "name": "nyaf",
  "version": "0.6.0",
  "description": "\"Not Yet Another Framework\" is, well, just another framework. It is, however, extremely simple to learn.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project ./src/packages/@nyaf/lib && tslint --project ./src/packages/@nyaf/forms && tslint --project ./src/packages/@nyaf/store",
    "nyaf": "export NODE_ENV=prod && webpack --config webpack.config.lib.js --env.NODE_ENV=prod --env.production",
    "nyaf-debug": "export NODE_ENV=dev && webpack --config webpack.config.lib.js --env.NODE_ENV=dev --env.development",
    "build": "rimraf dist/@nyaf && cp README.md ./src/packages/@nyaf/lib && rimraf out-tsc && tsc && npm run nyaf",
    "build-debug": "rimraf dist/@nyaf && rimraf out-tsc && tsc && npm run nyaf-debug",
    "start": "npm run start-features",
    "start-features": "webpack-dev-server --config webpack.config.features.js --open",
    "start-ui": "webpack-dev-server --config webpack.config.ui.js --open",
    "build-start-demo": "npm run build-demo && http-server ./dist/demo/features -o -p 9000",
    "build-demo": "webpack --config webpack.config.features.js --env.NODE_ENV=prod --env.production && webpack --config webpack.config.ui.js --env.NODE_ENV=prod --env.production ",
    "publish": "npm run build && cd dist/@nyaf/lib && npm publish --access public && cd ../../.. && cd dist/@nyaf/forms && npm publish --access public && cd ../../.. && cd dist/@nyaf/store && npm publish --access public && cd ../../../ && cd dist/@nyaf/cli && npm publish --access public",
    "doc": "rimraf dist/manual && rimraf dist/apidoc && npm run t-lib && npm run t-forms && npm run t-store && npm run t-ui && mkdocs build && showdown makehtml -i README.md -o dist/demo/features/readme.html ",
    "t-lib": "typedoc --name \"@nyaf library\" --out dist/apidoc/lib src/packages/@nyaf/lib",
    "t-forms": "typedoc --name \"@nyaf forms\" --out dist/apidoc/forms src/packages/@nyaf/forms",
    "t-store": "typedoc --name \"@nyaf store\" --out dist/apidoc/store src/packages/@nyaf/store",
    "t-ui": "typedoc --name \"@nyaf ui\" --out dist/apidoc/ui src/packages/@nyaf/ui",
    "publish-doc": "mkdocs gh-deploy",
    "all": "rimraf dist && npm run build && npm run build-demo && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joergkrause/nyaf.git",
    "directory": "dist/@nyaf/lib"
  },
  "keywords": [
    "web components",
    "vanilla JS",
    "es2015",
    "framework",
    "web app"
  ],
  "author": "Joerg <isageek> Krause",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joergkrause/nyaf/issues"
  },
  "homepage": "https://github.com/joergkrause/nyaf#readme",
  "devDependencies": {
    "@types/highlight.js": "^9.12.4",
    "@types/jest": "^26.0.0",
    "@types/node": "^12.12.26",
    "@types/prismjs": "^1.16.1",
    "@types/yargs": "^15.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "inquirer": "^7.2.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "mkdocs": "0.0.1",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "prismjs": "^1.20.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "showdown": "^1.9.1",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.2",
    "typedoc": "^0.17.7",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.9.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.1",
    "yeoman-generator": "^4.10.1"
  },
  "dependencies": {
    "@nyaf/lib": "file:./src/packages/@nyaf/forms",
    "@nyaf/forms": "file:./src/packages/@nyaf/forms",
    "@nyaf/store": "file:./src/packages/@nyaf/store",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "bootstrap": "^4.5.0",
    "chalk": "^4.1.0",
    "highlight.js": "^10.1.2",
    "jquery": "^3.5.1",
    "jquery-easing": "0.0.1",
    "meow": "^7.0.1",
    "popper.js": "^1.16.1",
    "uuid": "^8.1.0",
    "yargs": "^15.3.1"
  }
}
